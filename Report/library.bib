file = {:D$\backslash$:/Mendeley Desktop/Audio Engineering Society Conference 15th International Conference Audio, Acoustics {\&} Small Spaces/Vorl{\"{a}}nder - 1998 - Objective Characterization of Sound Fields in Small Rooms(2).pdf:pdf},
