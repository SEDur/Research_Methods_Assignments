file = {:D$\backslash$:/Mendeley Desktop/Acoustics, Speech, and Signal Processing, 1999. ICASSP '99. Proceedings., 1999 IEEE International Conference on/Rabenstein, Zayati - 1999 - A direct method to computational acoustics.pdf:pdf},
isbn = {0780350413},
journal = {Acoustics, Speech, and Signal Processing, 1999. ICASSP '99. Proceedings., 1999 IEEE International Conference on},
keywords = {acoustic field,acoustic signal processing,acoustic wave propagation,acoustical effects,active noise control,architectural acoustics,audio signal processing,computational acoustics,digital audio processing,digital simulation,direct method,dynamic 3D sound propagation,electro-acoustics,enclosure,exact numerical modelling,linear algebra,multidimensional system theory,multidimensional wave digital principles,numerical analysis,numerical solution,physics,robust algorithms,room acoustics,room acoustics model,simulation,sound field,wave equation,wave equations},
number = {4},
