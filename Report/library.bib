doi = {10.1121/1.4861243},
file = {:D$\backslash$:/drive/Uni Work/neg{\_}tec/Git/Negotiated{\_}Technical{\_}Module/woodworth.pdf:pdf},
issn = {0001-4966},
journal = {The Journal of the Acoustical Society of America},
